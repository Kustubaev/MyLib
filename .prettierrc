{
  "printWidth": 100,
  "tabWidth": 2,
  "tabs": true,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "trailingComma": "none",
  "noExtraBlankLines": true
}
